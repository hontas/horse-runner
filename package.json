    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "prettier": "^3.6.2",
    "typescript": "^5.2.2",
    "typescript-eslint": "^8.42.0",
    "vite": "^5.0.8",
    "vite-plugin-pwa": "^0.17.4",
    "workbox-window": "^7.0.0"
